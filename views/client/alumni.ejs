    <script>
      let ops = document.querySelectorAll(".toS");
      for (let option of ops) {
        option.addEventListener("click", () => {
          let sort = option.textContent;
          if (document.querySelector(".yearAdded")) {
            document.querySelector(".yearAdded").remove();
          }
          document.querySelector(".year").style.display = "flex";

          if (sort == "All") {
            document.querySelector(".option").style.display = "none";
            for (let header of document.querySelectorAll(".header")) {
              header.style.display = "flex";
            }
            for (let cards of document.querySelectorAll(".cards")) {
              cards.style.display = "flex";
            }
            document.querySelector(
              ".year"
            ).textContent = `<%= keys[0] %> - <%= (parseInt(keys[0]) + 1) - 2000 %>`;
          } else {
            let years = document.querySelectorAll(".year");

            for (let year of years) {
              if (sort == year.textContent.trim().slice(0, 4)) {
                let cardss = document.querySelectorAll(".cards");
                let headers = document.querySelectorAll(".header");
                for (let cards of cardss) {
                  cards.style.display = "none";
                }
                for (let header of headers) {
                  header.style.display = "none";
                }

                document.querySelector(".header").style.display = "flex";
                document.querySelector(".year").style.display = "none";

                let newYear = document.createElement("H2");
                newYear.classList.add("year");
                newYear.classList.add("yearAdded");
                newYear.textContent = `${year.textContent}`;
                document
                  .querySelector(".header")
                  .insertBefore(
                    newYear,
                    document.querySelector(".header").childNodes[2]
                  );

                let disCards = year.parentElement.nextElementSibling;
                disCards.style.display = "flex";
                document.querySelector(".option").style.display = "block";
              }
            }
          }
        });
      }
    </script>
