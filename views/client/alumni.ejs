      <% for (let i = 0; i < keys.length; i++) { %>
      <section class="header">
        <% if (i==0) { %>
        <div class="inputCont">
          <div class="input">
            <input id="searchBar" type="text" placeholder="Search" />
            <button>
              <img class="darkR" src="../assets/search.svg" alt="" /><img
                class="lightR"
                src="../assets/search2.svg"
                alt=""
              />
            </button>
          </div>
        </div>
        <% } %>
        <h2 class="year">
          <%= keys[i] %> - <%= (parseInt(keys[i]) + 1) - 2000 %>
        </h2>
        <% if (i==0) { %>
        <div class="sortCont">
          <div class="sort">
            <div class="by">Sort by Year</div>
            <div class="buttn">
              <i class="fas fa-sort-down"></i>
            </div>
            <div class="options">
              <div id="all" class="option toS">All</div>
              <% for (let year of keys) { %>
              <div class="option toS"><%= year %></div>
              <% } %>
            </div>
          </div>
        </div>
        <% } %>
      </section>

      <section class="cards">
        <% for (let alumni of obj[keys[i]]) { %>
        <div class="card">
          <div class="cardHeader">
            <img src="/img/<%= alumni.image %>" alt="" />
            <div class="cardRight">
              <h2><%= alumni.name %></h2>
              <h3><%= alumni.post %></h3>
            </div>
          </div>
          <!-- <p><%= alumni.current %></p> -->
          <div class="social">
            <% for (let social of alumni.socials) { %> <% if (social.platform ==
            "envelope") { %>
            <a href="mailto:<%= social.URL %>" target="_blank">
              <i class="fas fa-<%= social.platform %>"></i>
            </a>
            <% } else if (social.platform == "pager") { %>
            <a href="<%= social.URL %>" target="_blank">
              <i class="fas fa-<%= social.platform %>"></i>
            </a>
            <% } else { %>
            <a href="<%= social.URL %>" target="_blank">
              <i class="fab fa-<%= social.platform %>"></i>
            </a>
            <% } %> <% } %>
          </div>
        </div>
        <% } %>
      </section>

